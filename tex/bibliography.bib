@Article{Reams1999,
  author   = {Robert Reams},
  journal  = {Linear Algebra Appl.},
  title    = {Hadamard Inverses, Square Roots and Products of Almost Semidefinite Matrices},
  year     = {1999},
  issn     = {0024-3795},
  pages    = {35-43},
  volume   = {288},
  abstract = {Let A = (aij) be an n $\times$ n symmetric matrix with all
                  positive entries and just one positive eigenvalue. Bapat
                  proved then that the Hadamard inverse of A, given by
                  A$\deg$(-1) = (1aij) is positive semidefinite. We show that if
                  moreover A is invertible then A$\deg$(-1) is positive
                  definite. We use this result to obtain a simple proof that
                  with the same hypotheses on A, except that all the diagonal
                  entries of A are zero, the Hadamard square root of A, given by
                  A$\deg$12 = (aij12), has just one positive eigenvalue and is
                  invertible. Finally, we show that if A is any positive
                  semidefinite matrix and B is almost positive definite and
                  invertible then A ○ B ⪰ (1/eTB-1e)A.},
  doi      = {10.1016/S0024-3795(98)10162-3},
  file     = {:Reams1999.pdf:PDF;:notes/Reams1999.org:Org;:reference/Reams1999.org:Org;:notes/reference/Reams1999.org:Org},
  keywords = {Hadamard product, Hadamard square root, Hadamard inverse, Distance matrix, Positive semidefinite, Almost positive semidefinite},
}

