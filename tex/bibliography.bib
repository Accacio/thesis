@Article{LorenzenEtAl2019,
  author   = {Matthias Lorenzen and Mark Cannon and Frank Allg{\"o}wer},
  journal  = {Automatica},
  title    = {Robust Mpc With Recursive Model Update},
  year     = {2019},
  issn     = {0005-1098},
  pages    = {461-471},
  volume   = {103},
  abstract = {Robust constrained control of linear systems with
                  parametric uncertainty and additive disturbance is addressed.
                  The main contribution is the introduction of a mathematically
                  rigorous and computationally tractable framework for
                  stabilizing model predictive control with online parameter
                  estimation to improve performance and reduce conservatism.
                  Requirements for closed-loop stability and provable constraint
                  satisfaction are considered separately, resulting in the use
                  of online set-membership system identification combined with
                  homothetic prediction tubes for robust constraint
                  satisfaction, and an $H_{\infty}$ optimal point estimate of
                  the unknown parameters to achieve a finite closed-loop gain
                  from the disturbance to the state. Extensions to time-varying
                  parameters and persistently exciting inputs to guarantee
                  parameter convergence are presented. A numerical example
                  illustrates the proven properties and efficacy of the
                  approach.},
  doi      = {10.1016/j.automatica.2019.02.023},
  file     = {:LorenzenEtAl2019.pdf:PDF},
  keywords = {Model predictive control, Adaptive model predictive control, Receding horizon control, Control of constrained systems, Adaptive control, Uncertain linear systems, System identification},
}

