@InProceedings{AminEtAl2009,
  author          = {Amin, Saurabh and C{\'a}rdenas, Alvaro A. and Sastry, S.
                  Shankar},
  title           = {Safe and Secure Networked Control Systems under
                  Denial-of-Service Attacks},
  booktitle       = {Hybrid Systems: Computation and Control},
  year            = 2009,
  pages           = {31--45},
  doi             = {10.1007/978-3-642-00602-9\_3},
  abstract        = {We consider the problem of security constrained optimal
                  control for discrete-time, linear dynamical systems in which
                  control and measurement packets are transmitted over a
                  communication network. The packets may be jammed or
                  compromised by a malicious adversary. For a class of
                  denial-of-service (DoS) attack models, the goal is to find an
                  (optimal) causal feedback controller that minimizes a given
                  objective function subject to safety and power constraints. We
                  present a semi-definite programming based solution for solving
                  this problem. Our analysis also presents insights on the
