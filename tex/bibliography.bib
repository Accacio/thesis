@Article{LvEtAl2021,
  author   = {Lv, Feiya and Yu, Shujian and Wen, Chenglin and Principe, Jose C.},
  journal  = {J. Franklin Inst. B},
  title    = {Interpretable {Fault} {Detection} Using {Projections} of {Mutual} {Information} {Matrix}},
  year     = {2021},
  issn     = {0016-0032},
  month    = feb,
  abstract = {This paper presents a novel mutual information (MI) matrix
                  based method for fault detection. Given a m-dimensional fault
                  process, the MI matrix is a m$\times$m matrix in which the
                  (i,j)-th entry measures the MI values between the i-th
                  dimension and the j-th dimension variables. We introduce the
                  recently proposed matrix-based R{\'e}nyi's $\alpha$-entropy
                  functional to estimate MI values in each entry of the MI
                  matrix. The new estimator avoids density estimation and it
                  operates on the eigenspectrum of a (normalized) symmetric
                  positive definite (SPD) matrix, which makes it well suited for
                  industrial process. We combine different orders of statistics
                  of the transformed components (TCs) extracted from the MI
                  matrix to constitute the detection index, and derive a simple
                  similarity index to monitor the changes of characteristics of
                  the underlying process in consecutive windows. We term the
                  overall methodology "projections of mutual information matrix"
                  (PMIM). Experiments on both synthetic data and the benchmark
                  Tennessee Eastman process demonstrate the interpretability of
                  PMIM in identifying the root variables that cause the faults,
                  and its superiority in detecting the occurrence of faults in
                  terms of the improved fault detection rate (FDR) and the
                  lowest false alarm rate (FAR). The advantages of PMIM is also
                  less sensitive to hyper-parameters. The advantages of PMIM is
                  also less sensitive to hyper-parameters. Code of PMIM is
                  available at
                  https://github.com/SJYuCNEL/Fault\_detection\_PMIM.},
  doi      = {10.1016/j.jfranklin.2021.02.016},
  file     = {:LvEtAl2021.pdf:PDF},
  keywords = {Fault detection, Mutual information matrix, Matrix-based R{\'e}nyi's -entropy functional, Transformed component, Interpretability},
  language = {en},
  urldate  = {2021-02-25},
}

@Article{MaestreEtAl2021,
  author   = {Jos{\'e} M. Maestre and Pablo Velarde and Hideaki Ishii and Rudy R. Negenborn},
  journal  = {Control Eng Pract},
  title    = {Scenario-Based Defense Mechanism Against Vulnerabilities in Lagrange-Based Dmpc},
  year     = {2021},
  issn     = {0967-0661},
  pages    = {104879},
  volume   = {114},
  abstract = {In this paper, we present an analysis of the vulnerability
                  of a distributed model predictive control (DMPC) scheme in the
                  context of cyber-security. We consider different types of the
                  so-called insider attacks. In particular, we consider the
                  situation where one of the local controllers sends false
                  information to others to manipulate costs for its own
                  advantage. Then, we propose a popular scenario-based mechanism
                  to protect or, at least, relieve the consequences of the
                  attack in a typical DMPC negotiation process. The theoretical
                  and algorithmic properties of this defense mechanism are also
                  analyzed. A real case study based on a four tank plant is
                  provided to illustrate both the consequences of the attacks
                  and the defense mechanisms.},
  doi      = {10.1016/j.conengprac.2021.104879},
  file     = {:MaestreEtAl2021.pdf:PDF;:notes/MaestreEtAl2021.org:Org;:reference/MaestreEtAl2021.org:Org;:notes/reference/MaestreEtAl2021.org:Org},
  keywords = {Model predictive control, Distributed control, Multi-agent network, Cyber-security},
}

